{
  "name": "graphql-ts-server-boilerplate",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@graphql-codegen/core": "^2.6.2",
    "@graphql-codegen/typescript": "^2.7.3",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.18",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/nodemailer": "^6.4.5",
    "@types/sparkpost": "^2.1.5",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.0",
    "@graphql-tools/load": "^7.7.1",
    "@graphql-tools/schema": "^9.0.1",
    "axios": "^0.27.2",
    "axios-cookiejar-support": "^4.0.3",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "graphql": "^16.5.0",
    "graphql-yoga": "^2.13.6",
    "ioredis": "^5.2.2",
    "nodemailer": "^6.7.8",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sparkpost": "^2.1.4",
    "tough-cookie": "^4.0.0",
    "typeorm": "0.3.7",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "test-server": "NODE_ENV=test ts-node --files src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "test": "NODE_ENV=test jest --forceExit --verbose --runInBand",
    "gen-schema-types": "ts-node src/scripts/createTypes.ts"
  },
  "jest": {
    "globalSetup": "./src/testSetup/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
